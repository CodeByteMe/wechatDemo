<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.zj.wechat.mapper.TextContentMapper">
    <select id="getMsg" parameterType="string" resultType="TextContent">
        SELECT *
        FROM Msg_Text
        WHERE content = #{content}
    </select>
</mapper>